{
    "hosting": [
        {
            "target": "stable",
            "public": "dist/releases/mosaic-docs",
            "cleanUrls": true,
            "rewrites": [
                {
                    "source": "**/!(*.*)",
                    "destination": "/index.html"
                }
            ],
            "ignore": [
                "firebase.json",
                "**/node_modules/**",
                "tmp"
            ]
        },
